{
  "framework": "nextjs",
  "headers": [
    {
      "source": "/.well-known/bimi/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/svg+xml"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
